package Game;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("Get ready for the next battle!");
		Archer arch = new Archer("Archer", 70, 15);
		Warrior war = new Warrior("Warrior", 80, 5 );
		
		arch.attack(war);
		war.healUp(war);
		war.buffForAttack();
		war.attack(arch);
		arch.attack(war);
		
		
		
	}


}
